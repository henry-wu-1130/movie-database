{
  "name": "movie-database",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.17.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postbuild": "npm run sentry:upload",
    "sentry:upload": "sentry-cli releases new $SENTRY_RELEASE && sentry-cli releases files $SENTRY_RELEASE upload-sourcemaps .next --rewrite --url-prefix '~/_next' && sentry-cli releases finalize $SENTRY_RELEASE",
    "sentry:clean": "sentry-cli releases prune 10",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "release": "npx commit-and-tag-version"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@headlessui/react": "^2.2.4",
    "@sentry/nextjs": "^9.40.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-virtual": "^3.13.12",
    "accept-language": "^3.0.20",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "latest": "^0.2.0",
    "negotiator": "^1.0.0",
    "next": "15.3.5",
    "next-pwa": "^5.6.0",
    "react": "^19.0.0",
    "react-cookie": "^8.0.1",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.5",
    "@eslint/eslintrc": "^3",
    "@sentry/cli": "^2.50.0",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.20",
    "@types/minimatch": "^5.1.2",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-fetch": "^4.1.0",
    "cypress": "^14.5.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
